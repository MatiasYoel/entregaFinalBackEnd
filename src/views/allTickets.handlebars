<div class="card w-50 mx-auto">
    <h1 class="card-header" id="{{user.id}}">Tickets</h1>

    <div class="card-body">
        <h6 class="card-subtitle mb-2 text-muted">Usuario: {{user.name}}</h6>
        <div class="" id="divTickets">

        </div>
        {{#each result.products }}
        <div class="card my-2">
            <div class="card-body">
                <h5 class="card-title">Titulo: {{this._id.title}}</h5>
                <p class="card-text">Descripcion: {{this._id.description}}</p>
                <p class="card-text">Categoria: {{this._id.category}}</p>
                <p class="card-text">Cantidad: {{this.quantity}}</p>
            </div>
        </div>
        {{/each}}
    </div>

</div>

<script src="js/allTickets.js"></script>